{
    "headers": {
        "Authorization": "Bearer <api-key>",
        "Accept": "application/vnd.api+json"
    },
    "url": {
        "player": "https://api.pubg.com/shards/steam/players?filter[playerNames]=${player}",
        "seasons": "https://api.pubg.com/shards/steam/seasons",
        "player_season_stats": "https://api.pubg.com/shards/steam/players/${player}/seasons/",
        "player_season_stats_ranked": "https://api.pubg.com/shards/steam/players/${player}/seasons/${season}/ranked"
    },
    "platform-region": {
        "asia": "pc-as",
        "europe": "pc-eu",
        "Japan": "pc-jp"
    }
    
}